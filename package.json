{
  "name": "azure-next-auth-stack",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "migrate": "pnpm prisma generate && pnpm prisma migrate deploy",
    "postinstall": "prisma generate",
    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:integration": "vitest run integration/",
    "test:components": "vitest run app/",
    "test:all": "pnpm test:ci && pnpm test:e2e",
    "test:all:ci": "pnpm test:ci && pnpm test:e2e"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.0",
    "@azure/app-configuration": "^1.6.0",
    "@azure/identity": "^4.3.0",
    "@azure/storage-blob": "^12.18.0",
    "@azure/web-pubsub": "^1.2.0",
    "@prisma/client": "^5.19.0",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "axios": "^1.12.2",
    "next": "14.2.9",
    "next-auth": "4.24.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "24.3.2",
    "@types/react": "19.1.12",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "jsdom": "^25.0.1",
    "prisma": "^5.19.0",
    "typescript": "^5.6.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.1"
  }
}